#!/bin/sh
killall ping
killall log_to_file
sleep 3

xterm -e "~/linux-80211n-csitool-supplementary/netlink/log_to_file csi_ng_$1_$2.dat
" &
sleep 6
loggerPID=$(pgrep log_to_file)
renice -19 -p $loggerPID

xterm -e "ping -s 65000 -i 0.2 10.0.0.1
" &

sleep 30
killall ping
sleep 3
killall log_to_file

